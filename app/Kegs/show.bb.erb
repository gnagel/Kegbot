<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @kegs.name %></h1>
    <%= link_to "Edit", :action => :edit, :id => @kegs.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @kegs.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ImageUrl</td>
        <td class="itemValue"><%= @kegs.image_url%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">KegVolumeSizeMl</td>
        <td class="itemValue"><%= @kegs.keg_volume_size_ml%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">KegVolumeRemainingMl</td>
        <td class="itemValue"><%= @kegs.keg_volume_remaining_ml%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">KegVolumeSpilledMl</td>
        <td class="itemValue"><%= @kegs.keg_volume_spilled_ml%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Status</td>
        <td class="itemValue"><%= @kegs.status%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Url</td>
        <td class="itemValue"><%= @kegs.url%></td>
      </tr>
      
    </table>
  </div>

</div>
