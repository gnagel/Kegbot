<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @drinks.pour_timestamp %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
    <a href="<%= url_for :action => :edit, :id => @drinks.object %>" class="ui-btn-right" data-icon="star">
      Edit
    </a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">PourTimestamp</div>
            <div class="itemValue"><%= @drinks.pour_timestamp %></div>
          </li>
      
          <li>
            <div class="itemLabel">DurationSec</div>
            <div class="itemValue"><%= @drinks.duration_sec %></div>
          </li>
      
          <li>
            <div class="itemLabel">VolumeMl</div>
            <div class="itemValue"><%= @drinks.volume_ml %></div>
          </li>
      
          <li>
            <div class="itemLabel">Url</div>
            <div class="itemValue"><%= @drinks.url %></div>
          </li>
      
          <li>
            <div class="itemLabel">Ticks</div>
            <div class="itemValue"><%= @drinks.ticks %></div>
          </li>
      
          <li>
            <div class="itemLabel">SessionId</div>
            <div class="itemValue"><%= @drinks.session_id %></div>
          </li>
      
          <li>
            <div class="itemLabel">Status</div>
            <div class="itemValue"><%= @drinks.status %></div>
          </li>
      
    </ul>
  </div>

</div>
