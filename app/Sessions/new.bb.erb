<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Sessions</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @sessions.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Name: </td>
                <td class="itemValue"><input type="text" name="sessions[name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Start timestamp: </td>
                <td class="itemValue"><input type="text" name="sessions[start_timestamp]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">End timestamp: </td>
                <td class="itemValue"><input type="text" name="sessions[end_timestamp]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Slug: </td>
                <td class="itemValue"><input type="text" name="sessions[slug]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Url: </td>
                <td class="itemValue"><input type="text" name="sessions[url]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
