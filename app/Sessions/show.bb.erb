<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @sessions.name %></h1>
    <%= link_to "Edit", :action => :edit, :id => @sessions.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @sessions.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">StartTimestamp</td>
        <td class="itemValue"><%= @sessions.start_timestamp%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EndTimestamp</td>
        <td class="itemValue"><%= @sessions.end_timestamp%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Slug</td>
        <td class="itemValue"><%= @sessions.slug%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Url</td>
        <td class="itemValue"><%= @sessions.url%></td>
      </tr>
      
    </table>
  </div>

</div>
